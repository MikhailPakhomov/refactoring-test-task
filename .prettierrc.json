{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "arrowParens": "avoid",
    "endOfLine": "crlf",
    "importOrder": ["^[./](?!.*store/|.*.scss)", "store/", ".scss", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
